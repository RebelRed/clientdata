<table>
  <thead>
    <tr>
      <th>Selector</th>
      <th>Entity name</th>
      <th>Existing type</th>
      <th>Billing</th>
      <th>Credit</th>
      <th>Notes</th>
      <th>Disable person </th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
    <% @entities.each do |entity| %>
   
   <tr>
        <td><%= entity.selector %></td>
        <td><%= entity.entity_name %></td>
        <td><%= entity.existing_type %></td>
        <td><%= entity.billing %></td>
        <td><%= entity.credit %></td>
        <td><%= entity.notes %></td>
        <td><%= entity.disable_person %></td> </tr>
    <% end %>
  </tbody>
</table>